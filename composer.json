{
  "name": "effsoft/eff-theme-creative",
  "description": "",
  "keywords": ["effsoft","eff"],
  "homepage": "https://github.com/effsoft/eff-theme-creative",
  "type": "eff",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "~2.0.14",
    "bower-asset/startbootstrap-creative": "~v5.1.3",
    "effsoft/eff": "dev-master"
  },
  "require-dev": {

  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "autoload" : {
    "psr-4" : {
      "effsoft\\eff\\theme\\creative\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "effsoft\\eff\\theme\\creative\\" : "src/"
    }
  },
  "repositories": {
    "eff":{
      "type": "path",
      "url": "../eff"
